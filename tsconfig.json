{
    "compilerOptions": {
        "target": "ES2020",
        "module": "ES2020",
        "lib": [
            "ES2020",
            "DOM"
        ],
        "outDir": "Electrical Calc./dist",
        "rootDir": "./",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "declaration": true,
        "declarationMap": true,
        "sourceMap": true,
        "moduleResolution": "node"
    },
    "include": [
        "*.ts",
        "utils/**/*.ts"
, "Electrical Calc./main.ts"    ],
    "exclude": [
        "Electrical Calc./node_modules"
    ]
}